<% const is_ns = x => typeof x !== 'string'; %>

<%  const print = (exp, root) => {
      for (let k in exp) { %>
        <%= k %>: <%  if (is_ns(exp[k])) { %>
                        { <% print(exp[k], false) %> },
                  <%  } else { %>
                        require('<%= exp[k] %>')<%= root ? '' : `.${k}` %>,
                  <%  } %>
<%    }
    } %>

window.__________ = {
  <% print(exports, true) %>
};
